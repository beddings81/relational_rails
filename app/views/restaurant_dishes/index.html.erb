<%= link_to "Dishes", "/dishes" %> | <%= link_to "Restaurants", "/restaurants" %>
<h1><%= @restaurant.name %></h1>

<% @dishes.each do |dish| %>
  <h3><%= dish.name %></h3>
  <p>Id: <%= dish.id %></p>
  <p>Restaurant_id: <%= dish.restaurant_id %></p>
  <p>Price: <%= dish.price %></p>
  <p>Hot: <%= dish.hot %></p>
  <p>Created_at: <%= dish.created_at %></p>
  <p>Updated_at: <%= dish.updated_at %></p>
<% end %>

<%= link_to "Add Dish", "/restaurants/#{@restaurant.id}/dishes/new" %>